<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TerceiraActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/segunda_imagem" />

    <TextView
        android:id="@+id/textView_mx"
        android:layout_width="43dp"
        android:layout_height="25dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="11dp"
        android:layout_marginTop="166dp"
        android:text="M (x)"
        android:textColor="@color/colorPrimary"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="179dp" />

    <TextView
        android:id="@+id/textView_va"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="168dp"
        android:layout_marginStart="43dp"
        android:text="VA"
        android:textColor="@color/colorPrimary"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="318dp"
        tools:textColor="@color/colorPrimary" />

    <TextView
        android:id="@+id/textView_vb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView_va"
        android:layout_marginEnd="29dp"
        android:text="VB"
        android:textColor="@color/colorPrimary"
        tools:layout_editor_absoluteX="338dp"
        tools:layout_editor_absoluteY="318dp"
        tools:textColor="@color/colorPrimary" />




</RelativeLayout>